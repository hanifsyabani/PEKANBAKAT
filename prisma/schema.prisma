generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}


model Lomba{
  id Int @id @default(autoincrement())
  nama String
  nisn String
  kelas String
  ttl String
  jenisLomba String
  judulKarya String
  namaTim String?
  anggota String?
}

